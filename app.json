{
	"expo": {
		"name": "Quick Press",
		"slug": "Quick-Press",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./src/assets/icon/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./src/assets/splash/splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#000000"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package": "com.diaz.sergio.quickPressEXPO",
			"versionCode": 1,
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/icon/adaptive-icon.png",
				"backgroundColor": "#373737"
			}
		},
		"web": {
			"favicon": "./src/assets/icon/icon.png"
		},
		"primaryColor": "#1e1e1e"
	}
}
